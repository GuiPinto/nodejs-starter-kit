{
    "name": "nodejs-starter-kit",
    "version": "0.0.1",
    "description": "This repo is a starter kit for working with Node.js, Handlebars (both server and client side), SASS (through Stylus), and RequireJS.",
    "engines": {
        "node": "0.10.x"
    },
    "main": "web.js",
    "license": "MIT",
    "dependencies": {
        "bower": "1.3.9",
        "express": "4.8.5",
        "express-hbs": "0.7.11",
        "grunt": "0.4.5",
        "grunt-cli": "0.1.13",
        "grunt-contrib-handlebars": "0.8.0",
        "grunt-contrib-jasmine": "0.7.0",
        "grunt-contrib-stylus": "0.18.0",
        "grunt-jslint": "1.1.12"
    },
    "devDependencies": {
        "grunt-contrib-watch": "0.6.1",
        "grunt-notify": "0.3.1"
    },
    "scripts": {
        "postinstall": "./node_modules/bower/bin/bower install && ./node_modules/grunt-cli/bin/grunt default",
        "test": "grunt test"
    },
    "keywords": [
        "node",
        "bower",
        "express",
        "grunt",
        "handlebars",
        "jasmine",
        "stylus",
        "jslint"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/neogeek/nodejs-starter-kit.git"
    }
}
